{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"untitled": {
			"projectType": "application",
			"schematics": {},
			"root": "",
			"sourceRoot": "app",
			"prefix": "src",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"allowedCommonJsDependencies": [
							"fast-sha256",
							"qrcode"
						],
						"optimization": {
							"scripts": true,
							"styles": {
								"minify": false,
								"inlineCritical": false
							},
							"fonts": true
						},
						"outputPath": "dist/GLeaks",
						"index": "app/index.html",
						"main": "app/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "tsconfig.app.json",
						"assets": [
							"app/assets"
						],
						"styles": [
							"node_modules/bootstrap/dist/css/bootstrap.css",
							"app/css/main.css",
							"app/css/ui-select-bootstrap4-fix.css",
							"app/css/loader.css",
							"app/css/print.css",
							"app/css/button.css",
							"app/css/ng-select.css",
							"node_modules/font-awesome/css/font-awesome.css",
							"node_modules/@fortawesome/fontawesome-free/css/fontawesome.css",
							"node_modules/@fortawesome/fontawesome-free/css/solid.css",
							"app/assets/lib/css/solid.css",
							"node_modules/@ng-select/ng-select/themes/default.theme.css",
							"node_modules/ng-multiselect-dropdown/themes/ng-multiselect-dropdown.theme.scss",
							"app/css/bootstrap-fixes.css"
						],
						"scripts": [
							"node_modules/marked/marked.min.js"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"extractLicenses": false,
							"sourceMap": false
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": false,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": false
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "untitled:build:production"
						},
						"development": {
							"browserTarget": "untitled:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "untitled:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "tsconfig.spec.json",
						"assets": [
							"app/assets"
						],
						"styles": [
							"app/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"cli": {
		"analytics": false
	}
}
